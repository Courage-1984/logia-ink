/**
 * Cards Component
 * Service cards, project cards, icons, and all card-related styles
 *
 * Extracted from styles.css (lines: 558-792)
 */

/* Services Section */
.services-preview {
  position: relative;
  background: var(--bg-secondary);
  padding-top: 3rem !important;
  overflow: hidden;
}

/* Section Video Background Overlay */
.section-video-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
  overflow: hidden;
  object-fit: cover;
  object-position: center;
  background: rgba(10, 10, 10, 0.8);
  /* Background image loaded lazily via JavaScript */
  background-position: center;
  background-size: cover;
  background-repeat: no-repeat;
  transition: background-image 0.3s ease;
}

.section-video-background.bg-loaded {
  /* Background image has been loaded */
}

.background-video-container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  /* Performance optimizations */
  contain: layout style paint;
  will-change: contents;
}

.section-video-background video {
  position: absolute;
  top: 50%;
  left: 50%;
  min-width: 100%;
  min-height: 100%;
  width: auto;
  height: auto;
  transform: translate(-50%, -50%);
  /* Ensure video covers entire area */
  object-fit: cover;
  object-position: center;
  /* Performance optimizations */
  will-change: opacity;
  backface-visibility: hidden;
  transform-style: preserve-3d;
}

.section-video-background video {
  opacity: 0;
  transition: opacity 600ms ease;
}

.section-video-background video.is-ready {
  opacity: 1;
}

/* Section Video Overlay (for text readability) */
.section-video-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(
    135deg,
    rgba(10, 10, 10, 0.5) 0%,
    rgba(26, 26, 46, 0.45) 50%,
    rgba(22, 33, 62, 0.5) 100%
  );
  z-index: 1;
  pointer-events: none;
}

/* Ensure content is above video and overlay */
.services-preview .container {
  position: relative;
  z-index: 2;
}

.services-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 2rem;
}

/* Square Service Cards Grid */
.services-grid-square {
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
}

@media (min-width: 768px) {
  .services-grid-square {
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  }
}

@media (min-width: 1024px) {
  .services-grid-square {
    grid-template-columns: repeat(4, 1fr);
  }
}

/* Service Card */
.service-card {
  background: rgba(22, 33, 62, 0.5);
  border: 1px solid rgba(0, 255, 255, 0.1);
  padding: 2.5rem;
  border-radius: 10px;
  transition: all 0.3s ease;
  position: relative;
  overflow: hidden;
}

/* Square Service Card */
.service-card-square {
  background: rgba(22, 33, 62, 0.5);
  border: 1px solid rgba(0, 255, 255, 0.1);
  padding: 2rem;
  border-radius: 10px;
  transition: all 0.3s ease;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  aspect-ratio: 1;
  text-align: center;
}

.service-card-square .service-icon {
  margin-bottom: 1rem;
  flex-shrink: 0;
  position: relative;
}

/* Smooth infinite gradient animation for square card icons on hover */
.service-card-square:hover .service-icon svg {
  animation: iconGradientShift 4s ease-in-out infinite;
}

/* Animate stroke color through project colors with smooth transitions */
@keyframes iconGradientShift {
  0% {
    stroke: var(--accent-cyan);
    filter: drop-shadow(0 0 15px var(--glow-cyan));
  }

  20% {
    stroke: var(--accent-magenta);
    filter: drop-shadow(0 0 15px var(--glow-magenta));
  }

  40% {
    stroke: var(--accent-green);
    filter: drop-shadow(0 0 15px var(--glow-green));
  }

  60% {
    stroke: var(--accent-blue);
    filter: drop-shadow(0 0 15px var(--glow-blue));
  }

  80% {
    stroke: var(--accent-pink);
    filter: drop-shadow(0 0 15px var(--glow-pink));
  }

  100% {
    stroke: var(--accent-cyan);
    filter: drop-shadow(0 0 15px var(--glow-cyan));
  }
}

.service-card-square h3 {
  margin-bottom: 1.5rem;
  flex-grow: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: transform 0.3s ease;
}

/* Title grow effect on hover */
.service-card-square:hover h3 {
  transform: scale(1.2);
}

.service-card-square .service-modal-btn {
  margin-top: auto;
  width: 100%;
  max-width: 200px;
}

/* Square card hover effects */
.service-card-square.mouse-tilt-container:hover {
  border-color: var(--accent-cyan);
  box-shadow: 0 10px 40px rgba(0, 255, 255, 0.2);
}

.service-card-square::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(135deg, rgba(0, 255, 255, 0.1), rgba(255, 0, 255, 0.1));
  opacity: 0;
  transition: opacity 0.3s ease;
}

.service-card-square:hover::before {
  opacity: 1;
}

.offer-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 2.5rem;
}

.offer-panel {
  --panel-accent: var(--accent-cyan);
  --panel-glow: var(--glow-cyan);
  position: relative;
  padding: clamp(2rem, 4vw, 2.75rem);
  border-radius: 18px;
  background: linear-gradient(155deg, rgba(22, 33, 62, 0.92) 0%, rgba(10, 10, 10, 0.88) 100%);
  border: 1px solid rgba(255, 255, 255, 0.06);
  box-shadow: 0 30px 60px rgba(0, 0, 0, 0.35);
  overflow: hidden;
  display: flex;
  flex-direction: column;
  gap: 1.75rem;
  transition: border-color 0.3s ease, box-shadow 0.3s ease, transform 0.3s ease;
  opacity: 0;
  transform: translate3d(0, 40px, 0);
}

.offer-panel.is-visible {
  animation: offerPanelEnter 0.9s ease forwards;
}

.offer-panel::before,
.offer-panel::after {
  content: '';
  position: absolute;
  pointer-events: none;
  transition: transform 0.4s ease, opacity 0.4s ease;
}

.offer-panel::before {
  width: 65%;
  height: 65%;
  top: -35%;
  left: -35%;
  background: radial-gradient(circle at top left, var(--panel-accent) 0%, transparent 70%);
  opacity: 0.35;
}

.offer-panel::after {
  width: 45%;
  height: 45%;
  bottom: -25%;
  right: -25%;
  background: radial-gradient(circle at bottom right, var(--panel-glow) 0%, transparent 65%);
  opacity: 0.25;
}

.offer-panel:hover {
  border-color: var(--panel-accent);
  box-shadow: 0 25px 70px var(--panel-glow);
  transform: translateY(-6px);
}

.offer-panel:hover::before,
.offer-panel:hover::after {
  opacity: 0.55;
  transform: scale(1.08);
}

.offer-panel.offer-panel--cyan {
  --panel-accent: var(--accent-cyan);
  --panel-glow: var(--glow-cyan);
  --offer-enter-x: -80px;
}

.offer-panel.offer-panel--magenta {
  --panel-accent: var(--accent-magenta);
  --panel-glow: var(--glow-magenta);
  --offer-enter-x: 80px;
}

.offer-panel.offer-panel--green {
  --panel-accent: var(--accent-green);
  --panel-glow: var(--glow-green);
  --offer-enter-x: -100px;
}

.offer-panel.offer-panel--blue {
  --panel-accent: var(--accent-blue);
  --panel-glow: var(--glow-blue);
  --offer-enter-x: 100px;
}

.offer-panel.offer-panel--pink {
  --panel-accent: var(--accent-pink);
  --panel-glow: var(--glow-pink);
  --offer-enter-x: -120px;
}

.offer-panel.offer-panel--gold {
  --panel-accent: var(--accent-gold);
  --panel-glow: var(--glow-gold);
  --offer-enter-x: 120px;
}

.offer-panel.offer-panel--magenta:nth-of-type(7) {
  --offer-enter-x: 70px;
}

.offer-panel.offer-panel--cyan.offer-panel--right {
  --offer-enter-x: -70px;
}

/* Pricing cards */
.pricing-card {
  border: 1px solid rgba(255, 255, 255, 0.08);
  transition: border-color 0.3s ease, box-shadow 0.3s ease;
}

.pricing-card.pricing-card--cyan {
  border-color: rgba(0, 255, 255, 0.3);
}

.pricing-card.pricing-card--magenta {
  border-color: rgba(255, 0, 255, 0.3);
}

.pricing-card.pricing-card--magenta {
  border-color: var(--accent-magenta);
  box-shadow: 0 20px 60px var(--glow-magenta);
}

.pricing-card.pricing-card--green {
  border-color: rgba(0, 255, 0, 0.3);
}

.pricing-card.pricing-card--cyan.mouse-tilt-container:hover {
  border-color: var(--accent-cyan);
  box-shadow: 0 10px 40px var(--glow-cyan);
}

.pricing-card.pricing-card--magenta.mouse-tilt-container:hover {
  border-color: var(--accent-magenta);
  box-shadow: 0 10px 40px var(--glow-magenta);
}

.pricing-card.pricing-card--green.mouse-tilt-container:hover {
  border-color: var(--accent-green);
  box-shadow: 0 10px 40px var(--glow-green);
}

