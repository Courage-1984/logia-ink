@keyframes offerPanelEnter {
  0% {
    opacity: 0;
    transform: translate3d(var(--offer-enter-x, 0), 40px, 0) scale(0.96);
  }

  60% {
    opacity: 1;
    transform: translate3d(calc(var(--offer-enter-x, 0) * 0.25), -6px, 0) scale(1.01);
  }

  100% {
    opacity: 1;
    transform: translate3d(0, 0, 0) scale(1);
  }
}

